{
    "manifest_version": 2,
    "browser_action": {
        "default_title": "NopeCHA: CAPTCHA Solver",
        "default_icon": "icon/16.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
        "page": "page.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "@nopecha.com",
            "strict_min_version": "101.0"
        }
    }
}
